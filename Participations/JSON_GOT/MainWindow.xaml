<Window x:Class="JSON_GOT.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JSON_GOT"
        mc:Ignorable="d"
        Title="GOT Quote" Height="450" Width="800">
    <Window.Background>
        <ImageBrush ImageSource="https://coolmaterial.com/wp-content/uploads/2017/07/Tyrion-Dragon.jpg"/>
    </Window.Background>
    <Grid>
        <Button x:Name="btnGetQuote" Content="Get Quote" HorizontalAlignment="Left" Margin="161,229,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Height="73" Width="116" Click="btnGetQuote_Click">

        </Button>
        <TextBlock x:Name="txtQuote" HorizontalAlignment="Left" Margin="457,108,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="169" Width="333" Background="#B2FFFCFC"/>
        <Label x:Name="lblCharacter" Content="" HorizontalAlignment="Left" Margin="501,302,0,0" VerticalAlignment="Top" Width="289" Background="#99FDFBFB"/>
        <Button x:Name="btnExport" Content="Export Quotes" HorizontalAlignment="Left" Margin="240,330,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Height="73" Width="116" Click="btnExport_Click"/>


    </Grid>
</Window>
