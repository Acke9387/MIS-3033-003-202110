@model IEnumerable<MVC_Example1.Models.Student>

@{
    ViewBag.Title = "TAs";
}

<h2>TAs</h2>

<table class="table">
    <tr>
        <th>
            TA First Name
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SoonerID)
        </th>
        
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SoonerID)
        </td>
        
    </tr>
}

</table>
